{
    "name": "katsana/insurance-sdk-php",
    "description": "KATSANA Insurance Renewal SDK for PHP",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Katsana\\Insurance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Katsana\\Insurance\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "laravie/codex": "^4.0 || ^5.0",
        "laravie/codex-security": "^1.0.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.2",
        "nesbot/carbon": "^1.26.3 || ^2.0",
        "php-http/curl-client": "^1.7 || ^2.0",
        "phpunit/phpunit": "^7.5 || ^8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
